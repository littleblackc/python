
def add(a,b):
	return a+b

def remove(a,b):
	return a-b

def pop():
	pass


def mix_add(a,b):
	a = a+1
	b = b+2
	return a,b

print "1+2",add(1,2)
print "Non Function:",pop()

c = mix_add(1,2)
print "tuple", c

print c[0],c[1]


def power(x,n=2):
	s = 1
	while n > 0:
		n = n-1
		s = s* x
	return s 

print "power(5)", power(5)
print "power(5,3)",power(5,3)

def add_end(L=None):
	if L is None:
		L = []
	L.append("END")
	return L

print "add_end()",add_end()
print "add_end([1,2,3]",add_end([1,2,3])
